{
    "name": "wali/smart-seeder",
    "description": "A PHP/Laravel package for generating fake localized data (Arabic names, Hijri dates, Saudi IBAN, etc.).",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wali\\SmartSeeder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Wali-Eldin",
            "email": "creaspo6@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "laravel/pint": "^1.25",
        "rector/rector": "^2.1"
    },
    "require": {
        "php": "^8.2"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "pint",
        "format-test": "pint --test",
        "refactor": "rector process",
        "refactor-dry": "rector process --dry-run",
        "ci": [
            "@format-test",
            "@test",
            "@refactor-dry"
        ]
    }
}
